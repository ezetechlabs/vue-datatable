{
    "name": "iyogesharma/vue-datatable",
    "description": "package to help in handling server side",
    "version": "1.1",
    "keywords": [
        "iYogesharma",
        "element-ui",
        "vue-datatable",
        "datatable",
        "laravel",
        "element-table"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Yogesh Sharma",
        "email": "iyogesharma@gmail.com"
    }],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.*|6.*|7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0|~3.8.0|~3.9.0"
    },
    "autoload": {
        "psr-4": {
            "YS\\VueDatatable\\": "src/"
        },
        "files": [
            "src/Helpers/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "YS\\VueDatatable\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "YS\\VueDatatable\\DataTableServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
